{
  "name": "utilise",
  "version": "2.3.7",
  "description": "Lean JavaScript Utilities as Micro-libraries",
  "repository": {
    "type": "git",
    "url": "git://github.com/utilise/utilise.git"
  },
  "main": "index.js",
  "browser": {
    "colors": false,
    "pause": false,
    "send": false,
    "file": false,
    "via": false,
    "./pause": false,
    "./send": false,
    "./file": false,
    "./via": false
  },
  "scripts": {
    "minify": "uglifyjs utilise.js -m -c drop_console,dead_code -d client=true -o utilise.min.js",
    "gzip": "gzip -c utilise.min.js > utilise.min.js.gz",
    "build": "node --trace-warnings build && npm run minify && npm run gzip",
    "refresh": "npm update && npm run build",
    "version": "npm run build && git add -A",
    "postversion": "git push && git push --tags"
  },
